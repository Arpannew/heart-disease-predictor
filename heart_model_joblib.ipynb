    {
      "cell_type": "code",
      "source": [
        "# ---------- Cell 5: Fit Random Forest within a full pipeline ----------\n",
        "rf_clf = RandomForestClassifier(\n",
        "    n_estimators=400,\n",
        "    max_depth=None,\n",
        "    class_weight='balanced',\n",
        "    random_state=42,\n",
        "    n_jobs=-1\n",
        ")\n",
        "\n",
        "model = Pipeline(steps=[\n",
        "    ('preprocess', preprocess),\n",
        "    ('classifier', rf_clf)\n",
        "])\n",
        "\n",
        "model.fit(X_train, y_train)\n"
      ],
